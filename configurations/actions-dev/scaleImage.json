{
  "name": "scaleImage",
  "displayName": "Scale image",
  "description": "Scales an image to a given scale. ",
  "helpMessage": "",
  "icon": "scaleIcon.svg",
  "path": "/execute/scaleImage",
  "parameters": {
    "valuefields": [
      {
        "name": "width",
        "description": "The width of the image to scale to",
        "value": {
          "type": "integer",
          "min": 256,
          "max": 7680,
          "step": 1,
          "default": "$dynamic:maxImageWidth"
        }
      }
    ]
  }
}